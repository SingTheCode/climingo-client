{
  "name": "climingo-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H local.dev-app.climingo.xyz --experimental-https",
    "stg": "next dev -H local.stg-app.climingo.xyz --experimental-https",
    "prd": "next dev -H local.app.climingo.xyz --experimental-https",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "init": "bash ./start_shell",
    "test:unit": "jest --config jest.config.ts test/unit",
    "test:integration": "jest --config jest.config.ts test/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e"
  },
  "dependencies": {
    "@headlessui/react": "^2.0.3",
    "@tanstack/react-query": "^5.29.0",
    "axios": "^1.6.8",
    "dayjs": "^1.11.11",
    "mitt": "^3.0.1",
    "next": "14.1.1",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@tanstack/react-query-devtools": "^5.29.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^9.0.11",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "lint-staged": "^15.2.2",
    "playwright": "^1.54.1",
    "postcss": "^8",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": "eslint --fix"
  }
}
